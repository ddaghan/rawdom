{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": false,
        "sourceMap": false,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "../jsdist",
        "lib": [
            "es2016",
            "dom"
        ],
    },
    "include": [
        "./*"
    ],
    "exclude": [
        "node_modules"
    ]
}
